ServerAliveInterval 60

Host navi.localdomain
	HostName navi.localdomain
	ForwardAgent yes
	User christophe

Match exec "uname -s | grep -q Darwin"
	Include ~/.ssh/config.mac


Match exec "uname -s | grep -q Linux"
	Include ~/.ssh/config.linux


# jumphost to connect through for home network
Host tailscale
  HostName 100.92.157.19
  User root

Host *.localdomain
  ProxyJump tailscale
  ForwardAgent yes